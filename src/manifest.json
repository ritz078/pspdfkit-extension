{
  "manifest_version": 3,
  "name": "Chrome Extension with React & Webpack",
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "<all_urls>"],
      "js": ["contentScript.bundle.js"],
      "css": ["content.styles.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "content.styles.css",
        "icon-128.png",
        "icon-34.png",
        "*.js",
        "*.css",
        "*.wasm"
      ],
      "matches": ["http://*/*", "https://*/*", "<all_urls>"]
    }
  ]
}
